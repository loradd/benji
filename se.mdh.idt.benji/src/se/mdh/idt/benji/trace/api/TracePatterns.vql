package se.mdh.idt.benji.trace.api

// metamodels
import 'http://www.eclipse.org/emf/2002/Ecore'
import 'http://www.mdh.se/idt/benji/trace/Trace'

// trace link - initial element
pattern initial (traceLink : TraceLink, initial_element : EObject) {
	TraceLink.source (traceLink, initial_element); 
}

// trace link - current element
pattern current (traceLink : TraceLink, current_element : EObject) {
	TraceLink.target (traceLink, current_element); 	
}